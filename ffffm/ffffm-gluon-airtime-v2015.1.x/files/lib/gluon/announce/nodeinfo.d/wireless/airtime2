function file_exists(name)
 local f=io.open(name,"r")
 if f~=nil then
   io.close(f)
   return true
 else
   return false
 end
end

if (file_exists('/tmp/active2')) then
  local n = 0
  local active = util.trim(fs.readfile('/tmp/active2'))
  n = tonumber(active)
  local busy = util.trim(fs.readfile('/tmp/busy2'))
  m = tonumber(busy)
  return (m / n)
end
